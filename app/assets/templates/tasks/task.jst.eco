<%= @task.get('title') %>
<% if @task.get('completed'): %>
  <span class="completed btn">Done</span>
<% else: %>
  <span class="not completed btn btn-info"><a href="http://twitter.com/share?text=Hey everybody I finisht my task - <%= @task.get('title') %> - " target="_blank">Not Done</a></span>
<% end %>
<span class="delete">x</span>